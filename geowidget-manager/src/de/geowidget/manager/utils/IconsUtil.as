			public static function getFlagIcon(countryCode:String):Class			
			{
				
				countryCode = countryCode.toLowerCase();
				
				if(instance.hasOwnProperty(countryCode + 'FlagIcon'))
				{
					return instance[countryCode + 'FlagIcon'];
				}
				
				return null;
				
			}
			
	}
}